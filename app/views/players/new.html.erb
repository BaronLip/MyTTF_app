<div>
    <h1>Signup</h1>
</div>

<%= form_with model: @player do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br>
    <%= f.label :ranking %>
    <%= f.text_field :ranking %>
    <br>
    <%= f.label :password%>
    <%= f.password_field :password%>
    <br>
    <%= f.submit "Signup" %>
<% end %>